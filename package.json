{
  "name": "media-converter",
  "version": "0.1.0",
  "description": "this is a test app",
  "keywords": [
    "electron"
  ],
  "license": "MIT",
  "author": "fax1015",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dev:watch": "npx electronmon --trace-warnings .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.fax1015.videotoolbox",
    "productName": "Video Toolbox",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer/**/*",
      "assets/**/*"
    ],
    "extraResources": [
      {
        "from": "bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "portable",
      "icon": "assets/icons/favicon.ico"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0"
  }
}